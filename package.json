{
  "author": "Chad Wade Day, Jr. <cdayjr@chadwadedayjr.info>",
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        "@babel/preset-typescript",
        {
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "browser": "dist/video-tag.js",
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/cdayjr/video-tag/issues"
  },
  "description": "JavaScript to load third-party embed scripts for various video services.",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@microsoft/tsdoc": "^0.13.2",
    "@parcel/transformer-sass": "2.0.0-beta.3.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/dompurify": "^2.2.2",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.6",
    "concat-cli": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-xss": "^0.1.10",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-css-modules-transform": "^4.2.1",
    "jest-diff": "^27.0.2",
    "jest-environment-jsdom": "^27.0.3",
    "jest-serializer-html": "^7.0.0",
    "jsdom": "^16.6.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.0",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-modules": "^4.1.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.3.1",
    "rxjs": "^7.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.3",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.3.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "yaml": "^1.10.2"
  },
  "engines": {
    "pnpm": ">=6.7.6",
    "node": ">=14.17.0"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/cdayjr/video-tag",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "./coverage/",
    "globals": {
      "ts-jest": {
        "packageJson": "./package.json",
        "tsConfig": "./tests/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*.(ts|tsx|js)"
    ],
    "transform": {
      "\\.(css|scss|less|styl)$": "jest-css-modules-transform",
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{{t,j}son,,md,y{,a}ml,html}": [
      "prettier --write"
    ],
    "./{src,demo,tests}/**/*.{c,s{a,c},le}ss": [
      "prettier --write",
      "stylelint --fix --max-warnings=0",
      "tcm -c -p"
    ],
    "./{src,demo,tests}/**/*.{j,t}s": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "module": "lib/module.js",
  "name": "video-tag",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdayjr/video-tag.git"
  },
  "scripts": {
    "build": "npm run-script clean && npm run-script build-dist && npm run-script build-lib",
    "build-dist": "parcel build ./src/video-tag.ts && concat-cli -f copyright.js dist/video-tag.js -o dist/video-tag.js",
    "build-lib": "tsc --build tsconfig.lib.json && prettier --write \"./lib/**/*.js\" && copyfiles -u 1 src/**/*.scss lib/",
    "clean": "rm -rf ./dist ./lib",
    "demo": "npm run-script clean && parcel demo/index.html",
    "lint": "npm run-script ts-check && npm run-script stylelint && npm run-script eslint",
    "eslint": "eslint --fix --max-warnings=0 .",
    "stylelint": "stylelint --fix --max-warnings=0 ./{src,demo,tests}/**/*.{c,s{a,c},le}ss",
    "lint-staged": "npm run-script ts-check && lint-staged",
    "pre-commit": "npm run-script lint-staged && git add \"./src/**/*.scss.d.ts\"",
    "prepublishOnly": "npm run-script build",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "unpkg": "dist/video-tag.js",
  "version": "1.0.0",
  "dependencies": {
    "dompurify": "^2.2.9"
  }
}
