{
  "author": "Chad Wade Day, Jr. <cdayjr@chadwadedayjr.info>",
  "browser": "dist/video-tag.js",
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/cdayjr/video-tag/issues"
  },
  "description": "JavaScript to load third-party embed scripts for various video services.",
  "dependencies": {
    "@swc/helpers": "^0.5.1",
    "dompurify": "^3.0.5"
  },
  "engines": {
    "pnpm": "7.30.0",
    "node": "18.17.1"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/cdayjr/video-tag",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "./coverage/",
    "globals": {
      "ts-jest": {
        "packageJson": "./package.json",
        "tsconfig": "./tests/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*\\.(ts|tsx|js)"
    ],
    "transform": {
      "\\.(css|scss|less|styl)$": "jest-css-modules-transform",
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{{t,j}son,,md,y{,a}ml,html}": [
      "prettier --write"
    ],
    "./{src,demo,tests}/**/*.{c,s{a,c},le}ss": [
      "prettier --write",
      "stylelint --fix --max-warnings=0",
      "tcm -c -p"
    ],
    "./{src,demo,tests}/**/*.{j,t}s": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  },
  "module": "lib/module.js",
  "name": "video-tag",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdayjr/video-tag.git"
  },
  "scripts": {
    "build": "npm run-script clean && npm run-script build-dist && npm run-script build-lib",
    "build-dist": "parcel build ./src/video-tag.ts --log-level verbose && concat-cli -f copyright.js dist/video-tag.js -o dist/video-tag.js",
    "build-lib": "tsc --build tsconfig.lib.json && prettier --write \"./lib/**/*.js\" && copyfiles -u 1 src/**/*.scss lib/",
    "clean": "rm -rf ./dist ./lib",
    "demo": "npm run-script clean && parcel --log-level verbose demo/index.html",
    "lint": "npm run-script ts-check && npm run-script stylelint && npm run-script eslint",
    "eslint": "eslint --fix --max-warnings=0 .",
    "stylelint": "stylelint --fix --max-warnings=0 ./{src,demo,tests}/**/*.{c,s{a,c},le}ss",
    "lint-staged": "npm run-script ts-check && lint-staged",
    "pre-commit": "npm run-script lint-staged && git add \"./src/**/*.scss.d.ts\"",
    "prepublishOnly": "npm run-script build",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "unpkg": "dist/video-tag.js",
  "version": "1.0.0",
  "pnpm": {
    "overrides": {
      "postcss@<7.0.36": ">=7.0.36",
      "json5@<1.0.2": ">=1.0.2",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "pnpm@<7.33.4": ">=7.33.4",
      "loader-utils@<1.4.1": ">=1.4.1"
    }
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@parcel/core": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@tsconfig/node16": "^16.1.1",
    "@types/dompurify": "^3.0.2",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "^10.4.15",
    "concat-cli": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-xss": "^0.1.12",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-css-modules-transform": "^4.4.2",
    "jest-diff": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-serializer-html": "^7.1.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "node-sass": "^9.0.0",
    "parcel": "^2.9.3",
    "pnpm": "^8.7.0",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^9.1.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "yaml": "^2.3.1"
  }
}
